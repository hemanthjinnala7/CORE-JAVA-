// a class can inherit and implements  mutliple interfaces 
// where a class can inherit and extends only one abstract method

interface A {
    void run();
}
interface B{
    void walk();
}
interface C{
    void jog();
}
class All implements A,B,C{
    public void run()
    {
        System.out.println("running");
    }
    public void walk()
    {
        System.out.println("walking");
    }
    public void jog()
    {
        System.out.println("jogging");
    }
}

public class Main
{
    public static void main(String a[])
    {
        All all = new All();
        all.run();
        all.walk();
        all.jog();
    }
}
